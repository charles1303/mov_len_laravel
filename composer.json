{
	"name" : "laravel/movieLensLaravel",
	"type" : "project",
	"description" : "The Laravel Framework.",
	"keywords" : [
		"framework",
		"laravel",
		"movie"
	],
	"license" : "MIT",
	"require" : {
		"php": "^7.1.3",
		"fideloper/proxy": "^4.0",
		"guzzlehttp/guzzle": "^6.3",
		"laravel/framework": "5.7.*",
		"laravel/passport": "^7.1",
		"laravel/tinker": "^1.0",
		"predis/predis": "^1.1"
	},
	"require-dev" : {
		"beyondcode/laravel-dump-server" : "^1.0",
		"filp/whoops" : "^2.0",
		"fzaninotto/faker" : "^1.4",
		"mockery/mockery" : "^1.0",
		"nunomaduro/collision" : "^2.0",
		"phpunit/phpunit" : "^7.0"
	},
	"config" : {
		"optimize-autoloader" : true,
		"preferred-install" : "dist",
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "app/",
			"Age\\" : "app/Age/",
			"Movie\\" : "app/Movie/",
			"Auth\\" : "app/Auth/",
			"Rating\\" : "app/Rating/",
			"Exceptions\\" : "app/Exceptions/"
		},
		"classmap" : [
			"database/seeds",
			"database/factories",
			"app/Age/Models",
			"app/Age/Repositories",
			"app/Age/Services",
			"app/Movie/Models",
			"app/Movie/Repositories",
			"app/Movie/Services",
			"app/Auth/Models",
			"app/Auth/Repositories",
			"app/Auth/Services"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"scripts" : {
		"post-autoload-dump" : [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install" : "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
		"post-create-project-cmd" : "@php artisan key:generate --ansi",
		"test" : "vendor/bin/phpunit"
	}
}
